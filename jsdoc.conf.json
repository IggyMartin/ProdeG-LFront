{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false
    },
    "docdash": {
      "static": true,
      "search": true,
      "sort": true,
      "categorySort": "asc"
    }
  }
}

